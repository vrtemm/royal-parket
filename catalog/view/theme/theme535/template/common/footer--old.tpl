<script type="text/javascript">
	$(document).ready(function(){
		$('.common-home #content .box.bestsellers,.common-home #content .box.latest').css('display', 'none');
		$('.common-home #content .box.bestsellers .lazy img,.common-home #content .box.latest .lazy img').off("unveil");
		$('.common-home #content .box.bestsellers .image .lazy,.common-home #content .box.latest .image .lazy').addClass("lazy-loaded");
		
		$('.threein1 .trinone a').click(function(){
			$('.threein1 .trinone a').removeClass('active');
			$(this).addClass('active');
		});
		
		$('#mhspecial').click(function(){
			$('.box.specials').show();
			$('.box.bestsellers').hide();
			$('.box.latest').hide();
			return false;
		});
		$('#mhhit').click(function(){
			$('.box.bestsellers img').each(function() {
				var myatrsrc = $(this).attr("data-src");
				$(this).attr("src", myatrsrc);
			});
			$('.box.bestsellers').show();
			$('.box.specials').css('display', 'none');
			$('.box.latest').hide();
			return false;
		});
		$('#mhnew').click(function(){
			$('.box.latest img').each(function() {
				var myatrsrc = $(this).attr("data-src");
				$(this).attr("src", myatrsrc);
			});
			$('.box.latest').show();
			$('.box.bestsellers').hide();
			$('.box.specials').css('display', 'none');
			return false;
		});
	});
</script>
<script type="text/javascript">
	$('.city-item').on('click', function() {
		city = $(this).find('.selected-city').html();
		phone = $(this).find('.selected-city').data('phone');
		address = $(this).find('.selected-address').html();
		$(this).find('.selected-city').html($('#city-select > .selected-city').html());
		$(this).find('.selected-city').data('phone', $('#city-select > .selected-city').data('phone'));
		$(this).find('.selected-address').html($('#city-select > .selected-address').html());
		$('#city-select > .selected-city').html(city);
		$('#city-select > .selected-city').data('phone', phone);
		$('#city-select > .selected-address').html(address);
		$('.viewcity_phone').html(phone);
	})
</script>
